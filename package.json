{
  "name": "react-routing-resolver",
  "version": "1.0.2",
  "description": "Routing component, but It does't mount component. It's just resolver.",
  "keywords": [
    "component",
    "react",
    "resolve",
    "route",
    "router",
    "routing",
    "url"
  ],
  "homepage": "https://github.com/azu/react-routing-resolver",
  "bugs": {
    "url": "https://github.com/azu/react-routing-resolver/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/index.js",
  "types": "lib/react-routing-resolver.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/react-routing-resolver.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "prepublish": "npm run --if-present build",
    "test": "jest",
    "watch": "tsc -p . --watch"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "lib/"
    ]
  },
  "dependencies": {
    "@types/history": "^4.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "enroute": "^1.0.1",
    "history": "^4.2.0",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "cross-env": "^5.1.4",
    "jest": "^22.4.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.3.1",
    "ts-jest": "^22.4.2",
    "ts-node": "^5.0.1",
    "ts-node-test-register": "^2.0.0",
    "typescript": "^2.8.1"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
